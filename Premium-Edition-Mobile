page21:Ti("Stats")
page21:Toggle("Melee",_G.StatMelee,function(state)
	if state then
		_G.StatMelee = true
		while wait() do
			if _G.StatMelee == true then
				local args = {
					[1] = "Melee",
					[2] = _G.StatPoint
				}

				game:GetService("Players").LocalPlayer.PlayerGui.Stats.Button.StatsFrame.RemoteEvent:FireServer(unpack(args))
			end
		end
	else
		_G.StatMelee = false
	end
end)

