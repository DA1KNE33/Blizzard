	page13:Ti("Island")
	page13:DropDown("None",{
		"Starter Island",
		"Pirate Island",
		"Soldier Town",
		"Shark Island",
		"Chef Ship",
		"Snow Island",
		"Desert Island",
		"Sky Land",
		"Bubble Land",
		"Lobby Island",
		"Zombie Island",
		"War Island",
		"Fish Land",
		"Stone Arena [Shank]",
		"Stone Rain Sea"
	},function(value)
		if value == "Starter Island" then
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").SpawnPoints.Spawn1.CFrame
		elseif value == "Pirate Island" then
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").SpawnPoints.Spawn2.CFrame
		elseif value == "Soldier Town" then
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").SpawnPoints.Spawn3.CFrame
		elseif value == "Shark Island" then
