page32:Label2("")
page32:Button1("FPS Boost",function(t)
	local decalsyeeted = true
	local g = game
	local w = g.Workspace
	local l = g.Lighting
	local t = w.Terrain
	t.WaterWaveSize = 0
	t.WaterWaveSpeed = 0
	t.WaterReflectance = 0
	t.WaterTransparency = 0
	l.GlobalShadows = false
	l.FogEnd = 9e9
	l.Brightness = 0
	settings().Rendering.QualityLevel = "Level01"
	for i, v in pairs(g:GetDescendants()) do
		if v:IsA("Part") or v:IsA("Union") or v:IsA("CornerWedgePart") or v:IsA("TrussPart") then
			v.Material = "Plastic"
			v.Reflectance = 0
		elseif v:IsA("Decal") or v:IsA("Texture") and decalsyeeted then
			v.Transparency = 1
		elseif v:IsA("ParticleEmitter") or v:IsA("Trail") then
			v.Lifetime = NumberRange.new(0)
		elseif v:IsA("Explosion") then
			v.BlastPressure = 1
			v.BlastRadius = 1
		elseif v:IsA("Fire") or v:IsA("SpotLight") or v:IsA("Smoke") or v:IsA("Sparkles") then
			v.Enabled = false
		elseif v:IsA("MeshPart") then
			v.Material = "Plastic"
			v.Reflectance = 0
			v.TextureID = 10385902758728957
		end
	end	
end)

page32:Button1("Rejoin",function(t)
	game:GetService("TeleportService"):Teleport(game.PlaceId, game.Players.LocalPlayer)
end)
page32:Button1("Server Hop",function(t)
	Hop()
end)

page41:Ti("UI Setting")
page41:Toggle("Water Mark",_G.WaterMark,function(state)
	if state then
		_G.WaterMark = true
		if _G.WaterMark == true then
			game:GetService("CoreGui")["Blizzard Hub"].WaterMark.Visible = true
		end
	else
		_G.WaterMark = false
		if _G.WaterMark == false then
			game:GetService("CoreGui")["Blizzard Hub"].WaterMark.Visible = false
		end
	end
end)
page41:Ti("Discord")
page41:Button1("Join Discord", function(t)
	JoinDiscord()
end)
page41:Button1("Copy Discord Invite", function(t)
	setclipboard("https://discord.gg/fbXdWfCyNU")
end)

page42:Ti("Config")




game:GetService("Players").LocalPlayer.Idled:connect(function()
	game:GetService("VirtualUser"):Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
	wait(1)
	game:GetService("VirtualUser"):Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
end)

--BusoHaki
spawn(function()
	while wait(.1) do
		pcall(function()	
			--BusoHaki
			if _G.BusoHaki == true then
				if game:GetService("Players").LocalPlayer.Character.Haki.Value == 0 then
					game:GetService("VirtualInputManager"):SendKeyEvent(true,Enum.KeyCode.T,false,game) 
				end
			end
		end)
	end
end)
--KenHaki
spawn(function()
	while wait(.1) do
		pcall(function()	
			if _G.KenHaki == true then		
				if game:GetService("Players").LocalPlayer.Character.KenOpen.Value == false then
					game:GetService("VirtualInputManager"):SendKeyEvent(true,Enum.KeyCode.Y,false,game) 
				end
			end
		end)
	end
end)

--Walk on Water
spawn(function()
	while wait() do
		pcall(function()	
			if _G.WalkOnWater == true then

			end
		end)
	end
end)


--Bring Fruit
spawn(function()
	while wait() do
		pcall(function()
			if _G.BringFruit == true then
				for i,v in pairs(game:GetService("Workspace"):GetChildren()) do
					if v:IsA "Tool" then
						v.Handle.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
					end
				end	
			end
		end)
	end
end)


--Stat
spawn(function()
	while wait() do
		pcall(function()
			game:GetService("Players").LocalPlayer.PlayerGui.Stats.Button.StatsFrame.Number.Text = _G.StatPoint
		end)
	end
end)

--FPS Slider
spawn(function()
	while wait() do 
		pcall(function()
			setfpscap(_G.FpsValue)
		end)
	end
end)

--Quality Slider
spawn(function()
	while wait() do 
		pcall(function()
			settings().Rendering.QualityLevel = _G.QualityLevel
		end)
	end
end)

--Tpwalk Slider
spawn(function()
	while wait() do
		pcall(function()
			game:GetService("Players").LocalPlayer.Character:TranslateBy(game:GetService("Players").LocalPlayer.Character:FindFirstChild("Humanoid").MoveDirection * _G.TeleportWalkSpeed)
		end)
	end
end)



--Monster Status

spawn(function()
	while wait() do
		pcall(function()
			if game:GetService("Workspace").Island:FindFirstChild("Sea King Lava") or game:GetService("Workspace").Island:FindFirstChild("Sea King Water") or game:GetService("Workspace").Island:FindFirstChild("Sea King Thunder")then
				HydraStatus = "✅"
				HydraHealth = " Health "..game:GetService("Workspace").SeaMonster.HydraSeaKing.Humanoid.Health
			else 
				HydraStatus = "❌"
				HydraHealth = "| Not Spawn"
			end
			
			if game:GetService("Workspace").Island:FindFirstChild("Legacy Island1") or game:GetService("Workspace").Island:FindFirstChild("Legacy Island2") or game:GetService("Workspace").Island:FindFirstChild("Legacy Island3") or game:GetService("Workspace").Island:FindFirstChild("Legacy Island4")then 
				SeaKingStatus = "✅"
				SeaKingHealth = " Health "..game:GetService("Workspace").SeaMonster.SeaKing.Humanoid.Health
			else
				SeaKingStatus = "❌"
				SeaKingHealth = "| Not Spawn"
			end

			if game:GetService("Workspace").GhostMonster:FindFirstChild("Ghost Ship") then 
				GhostShipStatus = "✅"
				GhostShipHealth = " Health "..game:GetService("Workspace").GhostMonster["Ghost Ship"].Humanoid.Health
			else
				GhostShipStatus = "❌"
				GhostShipHealth  = "| Not Spawn"
			end

			if game:GetService("Workspace").Monster.Boss:FindFirstChild("King Samurai [Lv. 3500]") and game:GetService("ReplicatedStorage").MOB:FindFirstChild("King Samurai [Lv. 3500]") then
				KingSamuraiStatus = "✅"
				KingSamuraiStatus = "✅"
				KingSamuraiHealth = " Health "..game:GetService("Workspace").Monster.Boss["King Samurai [Lv. 3500]"].Humanoid.Health
				KingSamuraiHealth = " Health "..game:GetService("ReplicatedStorage").MOB["King Samurai [Lv. 3500]"].Humanoid.Health
			else
				KingSamuraiStatus = "❌"
				KingSamuraiHealth = "| Not Spawn"
			end

			if game:GetService("Workspace").Monster.Boss:FindFirstChild("Dragon [Lv. 5000]") or game:GetService("ReplicatedStorage").MOB:FindFirstChild("Dragon [Lv. 5000]") then
				KaidoStatus = "✅"
				KaidoStatus = "✅"
				KaidoHealth = " Health "..game:GetService("ReplicatedStorage").MOB["Dragon [Lv. 5000]"].Humanoid.Health
				KaidoHealth = " Health "..game:GetService("ReplicatedStorage").MOB["Dragon [Lv. 5000]"].Humanoid.Health
			else
				KaidoStatus = "❌"
				KaidoHealth = "| Not Spawn"
			end

			if game:GetService("Workspace").Monster.Boss:FindFirstChild("Ms. Mother [Lv. 7500]") or game:GetService("ReplicatedStorage").MOB:FindFirstChild("Ms. Mother [Lv. 7500]") then
				MrsMotherStatus = "✅"
				MrsMotherStatus = "✅"
				MrsMotherHealth = " Health "..game:GetService("ReplicatedStorage").MOB["Ms. Mother [Lv. 7500]"].Humanoid.Health
				MrsMotherHealth = " Health "..game:GetService("Workspace").Monster.Boss["Ms. Mother [Lv. 7500]"].Humanoid.Health
			else
				MrsMotherStatus = "❌"
				MrsMotherHealth = "| Not Spawn"
			end


			--Weapon Status

			if game:GetService("Players").LocalPlayer.Inventory:FindFirstChild("Hell Sword") then
				HellSwordStatus:Change("- Hell Swordㅤㅤㅤㅤ ㅤ ✅")
			else
				HellSwordStatus:Change("- Hell Swordㅤㅤㅤㅤ ㅤ ❌")
			end
			if game:GetService("Players").LocalPlayer.Inventory:FindFirstChild("Scepters of Flame") then
				ScepterofFlame1:Change("- Hell Swordㅤㅤㅤㅤ ㅤ ✅")
				ScepterofFlame2:Change("- Hell Swordㅤㅤㅤㅤ ㅤ ✅")
			else
				ScepterofFlame1:Change("- Hell Swordㅤㅤㅤㅤ ㅤ ❌")
				ScepterofFlame2:Change("- Hell Swordㅤㅤㅤㅤ ㅤ ❌")
			end
			if game:GetService("Players").LocalPlayer.Inventory:FindFirstChild("Authentic Triple Katana") then
				AuthenticTripleKatana:Change("- Hell Swordㅤㅤㅤㅤ ㅤ ✅")
			else
				AuthenticTripleKatana:Change("- Hell Swordㅤㅤㅤㅤ ㅤ ❌")
			end
			if _G.AimbotSkillsPlayers == true then
				AimbotPlayersText:Change("Aimbot Players ㅤ ✅")
			elseif _G.AimbotSkillsPlayers == false then
				AimbotPlayersText:Change("Aimbot Players ㅤ ❌")
			end

			--Aimbot

			if _G.AimbotSkillsMonsters == true then
				AimbotSkillsMonsters:Change("Aimbot Players ㅤ ✅")
			elseif _G.AimbotSkillsPlayers == false then
				AimbotSkillsMonsters:Change("Aimbot Players ㅤ ❌")
			end
			

			--Remove Fog
			if _G.RemoveFog == true then 
				game:GetService"Lighting".FogEnd = 100000
			end
			if _G.RemoveFog == false then 
				game:GetService"Lighting".FogEnd = 2500
			end
		end)
	end
end)

